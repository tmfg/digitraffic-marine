<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!--
        Temporary suppressions, check and update these periodically
    -->

    <suppress until="2022-09-15Z">
        <notes><![CDATA[
   file name: spring-*.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework.*$</packageUrl>
        <!-- RCE through deserialization vulnerability, not using vulnerable version of Spring -->
        <cve>CVE-2016-1000027</cve>

        <!-- JAXB marshalling XXE/CSRF vulnerability, not using vulnerable version of Spring -->
        <cve>CVE-2013-4152</cve>

        <!-- File upload vulnerability, not using file uploads -->
        <cve>CVE-2022-22970</cve>

        <!-- XXE vulnerability, not using vulnerable version of Spring -->
        <cve>CVE-2013-7315</cve>

        <!-- XXE/CSRF vulnerability, not using vulnerable version of Spring -->
        <cve>CVE-2014-0054</cve>

        <!-- RCE via data binding vulnerability, not using data binding.
        We are also using spring boot with executable jar which is not affected by this,. -->
        <cve>CVE-2022-22965</cve>

        <!-- Data binding field protection vulnerability, not using data binding.
        Not using vulnerable version of Spring. -->
        <cve>CVE-2022-22968</cve>

        <!-- Not using vulnerable version of Spring -->
        <cve>CVE-2018-11039</cve>

        <!-- Not using vulnerable version of Spring -->
        <cve>CVE-2018-11040</cve>

        <!-- Not using vulnerable version of Spring -->
        <cve>CVE-2018-1257</cve>

        <!-- Not using vulnerable version of Spring -->
        <cve>CVE-2020-5421</cve>

        <!-- Not using vulnerable version of Spring -->
        <cve>CVE-2022-22950</cve>
    </suppress>

    <suppress until="2022-09-15Z">
        <notes><![CDATA[
   file name: spring-security-crypto-*.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.security/spring\-security\-crypto@.*$</packageUrl>

        <!-- Crypto vulnerability, not using vulnerable version of Spring Security -->
        <cve>CVE-2020-5408</cve>
    </suppress>

    <suppress until="2022-09-15">
        <!-- From https://lists.apache.org/thread/k04zk0nq6w57m72w5gb0r6z9ryhmvr4k
             "Remove the examples web application as documented in the Tomcat security guide".

             This shouldn't be a problem when we use embedded tomcat provided by spring boot.
             Our tomcat does not expose the problematic examples. -->
        <notes><![CDATA[file name: tomcat-embed-*-9.0.63.jar]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.tomcat\.embed/tomcat\-embed\-(core|websocket)@.*$</packageUrl>
        <cve>CVE-2022-34305</cve>
    </suppress>

    <suppress until="2022-09-15">
        <!-- This is a false positive. This vulnerability is for "Caoyongqi912/Fan_Platform" and we are not using it.
             See: https://github.com/github/securitylab/issues/669#issuecomment-1189616056 -->
        <notes><![CDATA[file name: junit-platform-commons-1.8.2.jar]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\.platform/junit\-platform\-commons@.*$</packageUrl>
        <cve>CVE-2022-31514</cve>
    </suppress>

    <!--
        Permanent suppressions
    -->

    <!-- Not using Spring Security authorization -->
    <suppress>
        <notes><![CDATA[
   file name: spring-security-rsa-*.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.security/spring\-security\-rsa@.*$</packageUrl>
        <cve>CVE-2022-22978</cve>
    </suppress>

    <!-- Not using BCrypt -->
    <suppress>
        <notes><![CDATA[
   file name: spring-security-rsa-*.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.security/spring\-security\-rsa@.*$</packageUrl>
        <cve>CVE-2022-22976</cve>
    </suppress>

</suppressions>
